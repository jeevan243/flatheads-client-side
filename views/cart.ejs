<body>
  <%- include("partials/header.ejs") %>
      

    </header>
   
    <div class="main">
         <div id="cart_head"> 
            <h1>CART</h1>
        <button id="continue" onclick="cont()">Continue shopping</button>
        <ul id="list">
            <li>Price</li>
           <li>Quantity</li>
           <li>Total</li>
       </ul>  
        
        </div>
      <div id="cart_section"></div>
        <div id="sub_total">
         <div id="sub_text">SUBTOTAL</div>
        <div id="show_sub"></div>
        
        </div>  
        <div id="payment_opt">FREE shipping.Discount code can be applied on payment page.
        
        </div>  
        <!-- <img src="https://lh3.googleusercontent.com/nRkUyXf_7zH9AgKYBlEMC1FcR2dh-uq1T9d17oQr4S4WOrn2rFlRsG5UwwM-MIRNlCU1vdymri5Rr4hh01GOoremwmJennA2IUlQOjE" id="pay_img"> -->
         <div id="pay_img"> 
         
          <div id ="black">
            <div id="svg_img">
              <svg id="sv_image"width="28" height="28" fill="none"><path d="M19.648 4.107L13.664 11.5h4.834L6.123 24h9.127c4.492 0 7.37-2.354 7.907-5.527.479-2.95-1.338-4.766-3.603-4.854l.039-.195c2.129-.469 3.926-1.953 4.326-4.297.446-2.633-.953-4.595-4.29-5.034l.019.014z" fill="#fff"></path><path d="M17.59 4l-7.42 9.166h4.332L4.19 23.584 7.333 16.5H4L9 4h8.59z" fill="#fff"></path></svg>
